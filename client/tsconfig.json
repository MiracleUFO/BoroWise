{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*",
      ],
      "@context/*": ["context/*"],
      "@components/*": ["components/*"],
      "@containers/*": ["containers/*"],
      "@utils/*": ["utils/*"],
      "@constants/*": ["constants/*"],
    }
  },
  "include": [
    "*.eslintrc.cjs",
    "*.ts",
    "*.tsx",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "containers"  ]
}
